 poke(0x5f2c,3)
 cls()
end

function _update()
m = false --to stop double move

if btn (1) and btn (2) then 
		player.dir = 1
		player.x += 1
		player.y -= 1
		m = true
	end
	if btn (1) and btn (3) then 
		player.dir = 5
		player.x += 1
		player.y += 1
		m = true 
	end
	if btn (0) and btn (2) then 
		player.dir = 10
		player.x -= 1
		player.y -= 1
		m = true 
	end
	if btn (0) and btn (3) then 
		player.dir = 7
		player.x -= 1
		player.y += 1 
		m = true
	end
	if (m== false) then
		if btn(0) then 
			player.dir = 9
			player.x -= 1 
		end
		if btn(1) then 
			player.dir = 3 
			player.x += 1
		end
		if btn(2) then 
			player.dir = 12
			player.y -= 1
	 end
		if btn(3) then 
			player.dir = 6
			player.y += 1 
		end

	end


end

function _draw()
	cls()
	sspr(80,0,8,8, player.x-50, player.y-50, 120, 120)
	print ("return to mission area")
	mapdraw(0,0,0,0,128,32)
	camera(player.x-32, player.y-32)
	drawcopter(player.x,player.y)
	
end

s = 67
s2 = 83

function drawcopter(x,y)

	if player.dir == 12 then 
		spr(98, x, y)
	end
	if player.dir == 1 then 
		spr(97, x, y)
	end
	if player.dir == 3 then 
		spr(99, x, y)
	end
	if player.dir == 5 then 
		spr(96, x, y)
	end
	if player.dir == 6 then 
		spr(100, x, y)
	end
	
	if player.dir == 9 then 
		spr(99,x,y,1,1,true,false)
	end
	if player.dir == 7 then 
		spr(96,x,y,1,1,true,false)
	end
		if player.dir == 10 then 
		spr(97,x,y,1,1,true,false)
	end
	
	if player.dir == 3 or player.dir == 9 then
		spr (s2,x,y) --rotor
	else
		spr (s,x,y) --rotor
	end
	

if s == 72 then 
	s = 67 
	else s += 1 
end 

