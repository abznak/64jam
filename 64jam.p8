pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
player = {
	x=32 - 4, 
	y=32 - 4, 
	dir=12
}

enemies={}
pickups={}

function _init()
 poke(0x5f2c,3)
 cls()
 --init_entities()
end

function init_entities()
	for i=1,128 do 
		for j=1,32 do
		--loop through map and exchange tiles for entities
			if mget(i,j) == 10 then
				mset(i,j,1)
				
			end
		end
	end
end

function _update()
m = false --to stop double move

if btn (1) and btn (2) then 
		player.dir = 1
		player.x += 1
		player.y -= 1
		m = true
	end
	if btn (1) and btn (3) then 
		player.dir = 5
		player.x += 1
		player.y += 1
		m = true 
	end
	if btn (0) and btn (2) then 
		player.dir = 10
		player.x -= 1
		player.y -= 1
		m = true 
	end
	if btn (0) and btn (3) then 
		player.dir = 7
		player.x -= 1
		player.y += 1 
		m = true
	end
	if (m== false) then
		if btn(0) then 
			player.dir = 9
			player.x -= 1 
		end
		if btn(1) then 
			player.dir = 3 
			player.x += 1
		end
		if btn(2) then 
			player.dir = 12
			player.y -= 1
	 end
		if btn(3) then 
			player.dir = 6
			player.y += 1 
		end
	end
	
	update_projectiles()
	
	if btn(4) then fire_bullet() end
end

rockets = {}
bullets = {}

function update_projectiles()
	for b in all(bullets) do
		b.x += b.dx
		b.y += b.dy
	end
	
	for b in all(bullets) do
	
	end
end

function fire_bullet()
	x = 0
	y = 0
	
	if player.dir == 12 or player.dir == 10 or player.dir == 1 then
		y = -3
	end
	
	if player.dir == 5 or player.dir == 6 or player.dir == 7 then
	y = 3
	end
	 
	if player.dir >= 1 and player.dir < 6 then
		x = 3
	end
	
	if player.dir >= 6 and player.dir <= 10 then
		x = -3
	end 
	 
	local b={
		sp = 3,
		x = player.x,
		y = player.y,
		dx = x,
		dy = y
	}
	add(bullets, b)
end

function draw_projectiles()
	for r in all(rockets) do
		spr(112,r.x, r.y)
	end
	
	for b in all(bullets) do
		spr(112,b.x, b.y)
	end
end

function _draw()
	cls()
	sspr(80,0,8,8, player.x-50, player.y-50, 120, 120)
	mapdraw(0,0,0,0,128,64)
	camera(player.x-32, player.y-32)
	draw_projectiles()
	drawcopter(player.x,player.y)
end

s = 67
s2 = 83

function drawcopter(x,y)
	if player.dir == 12 then 
		spr(98, x, y)
	end
	if player.dir == 1 then 
		spr(97, x, y)
	end
	if player.dir == 3 then 
		spr(99, x, y)
	end
	if player.dir == 5 then 
		spr(96, x, y)
	end
	if player.dir == 6 then 
		spr(100, x, y)
	end
	
	if player.dir == 9 then 
		spr(99,x,y,1,1,true,false)
	end
	if player.dir == 7 then 
		spr(96,x,y,1,1,true,false)
	end
		if player.dir == 10 then 
		spr(97,x,y,1,1,true,false)
	end
	
	if player.dir == 3 or player.dir == 9 then
		spr (s2,x,y) --rotor
	else
		spr (s,x,y) --rotor
	end
	

if s == 72 then 
	s = 67 
	else s += 1 
end 

if s2 == 88 then 
	s2 = 83 
	else s2 += 1 
end

end
